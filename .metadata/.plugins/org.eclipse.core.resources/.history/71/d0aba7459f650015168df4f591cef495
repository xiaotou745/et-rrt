package com.renrentui.core.enums;

public enum WithdrawState  {
	/**
	 * 商家
	 */
	Business(1, "门店"),
	/**
	 * 骑士
	 */
	Clienter(2, "骑士");

	private int value = 0;
	private String desc;
	private WithdrawState(int value, String desc) { 
		this.value = value;
		this.desc = desc;
	}
	public int value() {
		return this.value;
	}
	public String desc() {
		return this.desc;
	}

	public static WithdrawState getEnum(int index) {
		for (WithdrawState c : WithdrawState.values()) {
			if (c.value() == index) {
				return c;
			}
		}
		return null;
	}
}

